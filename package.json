{
  "name": "@djforth/redux-helpers",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js' -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "asset-pipeline-karma",
    "test:mac": "asset-pipeline-karma -b",
    "test:watch": "asset-pipeline-karma -w"
  },
  "repository": {
    "url": "git+https://github.com/djforth/redux-helpers.git",
    "type": "git"
  },
  "keywords": [],
  "author": "Adrian Stainforth <adrian.stainforth@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/redux-helpers/issues"
  },
  "homepage": "https://github.com/djforth/redux-helpers#readme",
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "env": {
      "karma": {
        "plugins": [
          "add-module-exports",
          [
            "istanbul",
            {
              "exclude": [
                "**/*_spec.js"
              ]
            }
          ],
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "morsedigital",
    "globals": {
      "fdescribe": false,
      "describe": false,
      "beforeEach": false,
      "afterEach": false,
      "it": false,
      "expect": false,
      "spyOn": false,
      "I18n": false,
      "jasmine": false
    },
    "rules": {
      "require-jsdoc": 0
    }
  },
  "devDependencies": {
    "@djforth/ap_karma_wp": "^1.0.2",
    "@djforth/ap_utils": "^3.0.1",
    "@djforth/jasmine-call-helpers": "^1.0.0",
    "@djforth/morse-jasmine-wp": "^2.4.2",
    "@djforth/redux-jasmine-helper": "^1.0.2",
    "@djforth/stubs-spy-manager": "^1.1.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.0",
    "eslint-config-morsedigital": "^1.1.0",
    "eslint-plugin-react": "^6.4.1",
    "lodash": "^4.16.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "@djforth/ajax-es6-fp": "^1.1.9",
    "immutable": "^3.8.1"
  }
}
